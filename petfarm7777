local args = {
    [1] = "MainMap",
    [2] = "MainDoor",
    [3] = {
        ["spawn_cframe"] = CFrame.new(-275.9091491699219, 25.812084197998047, -1548.145751953125) * CFrame.Angles(3.1414573192596436, 0.20122411847114563, -3.1415693759918213)
    }
}

game:GetService("ReplicatedStorage").API:FindFirstChild("AdoptAPI/SendPassiveDoorEnter"):FireServer(unpack(args))

end







