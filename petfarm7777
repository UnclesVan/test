-- Get the Players service
local Players = game:GetService("Players")

-- Get the local player
local localPlayer = Players.LocalPlayer -- Gets the player this script belongs to.

-- Create a platform
local platform = Instance.new("Part")
platform.Size = Vector3.new(512, 1, 512)
platform.Anchored = true -- So it doesn't fall
platform.BrickColor = BrickColor.new("Bright green")
platform.Position = Vector3.new(5000, 500, 5000)
platform.Parent = workspace

-- Function to teleport the player
local function teleportPlayer() -- Added a function so we can make sure things are loaded first
    local character = localPlayer.Character or localPlayer.CharacterAdded:Wait() -- Wait for the character to load if it hasn't already.
    if character and character:FindFirstChild("HumanoidRootPart") then -- Check if the character is valid and has a HumanoidRootPart
        character:SetPrimaryPartCFrame(platform.CFrame + Vector3.new(0, 3, 0))  --Teleport the player to the platform
        print("Teleported to the Useless Platform.")
    else
        print("Character or HumanoidRootPart not found.  Could not teleport.") --Informs player of an error if it can't teleport.
    end
end

-- Call the teleport function immediately
teleportPlayer()

-- OPTIONAL:  Teleport the player again if they respawn
localPlayer.CharacterAdded:Connect(function()
    teleportPlayer()
end)



